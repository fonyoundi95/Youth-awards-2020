{% extends 'admin/base.html.twig' %}


{% block title %}
	Mantor
{% endblock %}

{% block body %}
	<h1 class="mt-5">
		Nos Mantors
	</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>
					Id
				</th>
				<td>
					{{ mantor.id }}
				</td>
			</tr>
			<tr>
				<th>
					Image
				</th>
				<td>
					{{ mantor.image }}
				</td>
			</tr>
			<tr>
				<th>
					Title
				</th>
				<td>
					{{ mantor.title }}
				</td>
			</tr>
			<tr>
				<th>
					Descriptions
				</th>
				<td>
					{{ mantor.descriptions }}
				</td>
			</tr>
		</tbody>
	</table>

	<a class="btn btn-info" href="{{ path('mantor_index') }}">back to list</a>

	<a class="btn btn-success" href="{{ path( 'mantor_edit', { id: mantor.id } ) }}">
		edit
	</a>

{{ include('admin/mantor/_delete_form.html.twig') }}

{% endblock %}
